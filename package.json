{
  "name": "hohma-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,js,json --exec tsx -r dotenv/config src/server.ts",
    "build": "tsc",
    "start": "pm2 start --no-daemon --node-args=\"-r dotenv/config\" dist/server.js",
    "start:ts": "tsx -r dotenv/config src/server.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "swagger": "node scripts/swagger-autogen.js"
  },
  "imports": {
    "#config/*": "./src/config/*",
    "#bot/*": "./src/bot/*",
    "#middleware/*": "./src/middleware/*",
    "#models/*": "./src/models/*",
    "#routes/*": "./src/routes/*",
    "#services/*": "./src/services/*",
    "#swagger/*": "./src/swagger/*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.750.0",
    "@riqtu/tiktok-scraper": "^1.0.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "instagram-private-api": "^1.46.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.46",
    "mongoose": "^8.9.2",
    "node-schedule": "^2.1.1",
    "socket.io": "^4.8.1",
    "socks-proxy-agent": "^8.0.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "telegraf": "^4.16.3",
    "uuid": "^10.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.5",
    "@types/node-schedule": "^2.1.7",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "swagger-autogen": "^2.23.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3"
